    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "db:types": "./bin/r-backups execute --database \"$DB_NAME\" --file ./src/database/types.sql",
        "db:init": "./bin/r-backups execute --database \"$DB_NAME\" --file ./src/database/init.sql",
        "db:functions": "./bin/r-backups execute --database \"$DB_NAME\" --file ./src/database/functions.sql",
        "db:triggers": "./bin/r-backups execute --database \"$DB_NAME\" --file ./src/database/triggers.sql",
        "db:indexs": "./bin/r-backups execute --database \"$DB_NAME\" --file ./src/database/indexs.sql",
        "db:create": "(set -a && . ./.env && set a+ && r-backups createdb --name \"$DB_NAME\" && npm run db:types && npm run db:init && npm run db:functions && npm run db:triggers && npm run db:indexs)"
    },
